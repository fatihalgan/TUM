<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>rolesManagerBean</managed-bean-name>
  <managed-bean-class>mcel.tump.view.users.RolesManagerBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
  	<property-name>securityService</property-name>
  	<value>#{securityService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>usersManagerBean</managed-bean-name>
 	<managed-bean-class>mcel.tump.view.users.UsersManagerBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 	<managed-property>
 		<property-name>securityService</property-name>
 		<value>#{securityService}</value>
 	</managed-property>
 	<managed-property>
 		<property-name>dealerService</property-name>
 		<value>#{dealerService}</value>
 	</managed-property>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>authorizationsManagerBean</managed-bean-name>
 	<managed-bean-class>mcel.tump.view.users.AuthorizationsManagerBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 	<managed-property>
 		<property-name>securityService</property-name>
 		<value>#{securityService}</value>
 	</managed-property>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>parametersManagerBean</managed-bean-name>
 	<managed-bean-class>mcel.tump.view.parameters.ParametersManagerBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 	<managed-property>
 		<property-name>parametersService</property-name>
 		<value>#{parametersService}</value>
 	</managed-property>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>dealersManagerBean</managed-bean-name>
 	<managed-bean-class>mcel.tump.view.dealers.DealersManagerBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 	<managed-property>
 		<property-name>dealerService</property-name>
 		<value>#{dealerService}</value>
 	</managed-property>
 	<managed-property>
 		<property-name>accountService</property-name>
 		<value>#{accountService}</value>
 	</managed-property>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>ipAddressesManagerBean</managed-bean-name>
 	<managed-bean-class>mcel.tump.view.dealers.IPAddressesManagerBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 	<managed-property>
 		<property-name>dealerService</property-name>
 		<value>#{dealerService}</value>
 	</managed-property>
 </managed-bean>
 <managed-bean>
 	<managed-bean-name>blacklistManagerBean</managed-bean-name>
 	<managed-bean-class>mcel.tump.view.dealers.BlacklistManagerBean</managed-bean-class>
 	<managed-bean-scope>session</managed-bean-scope>
 	<managed-property>
 		<property-name>parametersService</property-name>
 		<value>#{parametersService}</value>
 	</managed-property>
 </managed-bean>
 <navigation-rule>
  <navigation-case>
  	<from-outcome>listRoles</from-outcome>
  	<to-view-id>/faces/users/roles.xhtml</to-view-id>
  	<redirect/>
  </navigation-case>
  <navigation-case>
  	<from-outcome>listUsers</from-outcome>
  	<to-view-id>/faces/users/users.xhtml</to-view-id>
  	<redirect/>
  </navigation-case>
  <navigation-case>
  	<from-outcome>createShopUser</from-outcome>
  	<to-view-id>/faces/users/create_shop_user.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>listAuthorizations</from-outcome>
  	<to-view-id>/faces/users/authorizations.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>listParameters</from-outcome>
  	<to-view-id>/faces/parameters/trans_tresholds.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>listDealers</from-outcome>
  	<to-view-id>/faces/dealers/dealers.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>editDealer</from-outcome>
  	<to-view-id>/faces/dealers/edit_dealer.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>creditDealer</from-outcome>
  	<to-view-id>/faces/dealers/credit_dealer.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>listIPAddresses</from-outcome>
  	<to-view-id>/faces/dealers/ip_addresses.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
  	<from-outcome>listBlacklistedItems</from-outcome>
  	<to-view-id>/faces/dealers/blacklisted_dealers.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <validator>
  <validator-id>EmailValidator</validator-id>
  <validator-class>mcel.tump.util.jsf.EmailValidator</validator-class>
 </validator>
 <validator>
  <validator-id>MSISDNValidator</validator-id>
  <validator-class>mcel.tump.util.jsf.MSISDNValidator</validator-class>
 </validator>
 <application>
  <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <message-bundle>Messages</message-bundle>
 </application>
</faces-config>
